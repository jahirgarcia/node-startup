{
  "name": "node-startup",
  "version": "1.0.0",
  "description": "REST api startup template con nodejs",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel:node": "babel-node",
    "start": "nodemon --exec npm run babel:node src/server.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --out-dir dist",
    "prepare": "husky install",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahirgarcia/node-startup.git"
  },
  "author": "Jahir Garc√≠a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jahirgarcia/node-startup/issues"
  },
  "homepage": "https://github.com/jahirgarcia/node-startup#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
